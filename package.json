{
  "name": "vugarsafarzada-dev",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "docker-run:dev": "docker compose -f docker-compose.dev.yml up --build -d",
    "docker-run:build": "docker compose -f docker-compose.build.yml up --build -d",
    "docker-down:dev": "docker compose -f docker-compose.dev.yml down",
    "docker-down:build": "docker compose -f docker-compose.build.yml down",
    "docker-logs:dev": "docker compose -f docker-compose.dev.yml logs -f",
    "docker-logs:build": "docker compose -f docker-compose.build.yml logs -f"
  },
  "devDependencies": {
    "@nx/eslint": "22.4.5",
    "@nx/jest": "22.4.5",
    "@nx/next": "22.4.5",
    "eslint": "~8.57.0",
    "jest": "^30.0.2",
    "nx": "22.4.5",
    "prisma": "^6.19.2"
  },
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "dotenv": "^17.2.4",
    "next": "~16.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
